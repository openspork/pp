Config info for:
{{ mac_address }}
<br />

<ul>
	{# key = param, value = (value, depth, overrides) #}
	{% for key, value in rsop.items() %}
		{# if at depth 0, this is a phone #}
		{% if value[2] == 0 %} 
			<li>{{ key }} = {{ value[0] }} from <a href='{{ url_for('edit_phone', id=value[1] ) | safe }}'>phone</a> at depth {{ value[2] }}
			{% if value[3] | length > 0 %} </li>
				Overrode:
				<ul>
					{% for overridden_value, overridden_group, overriden_depth in value[3] %}
						<li>{{ key }} = {{ overridden_value }} from <a href='{{ url_for('edit_group', id=overridden_group ) | safe }}'>group</a> at depth {{ overriden_depth }}</li>
					{% endfor %}
				</ul>
			{% endif %}
		{% else %} {# if > depth 0, this is a group #}
			<li>{{ key }} = {{ value[0] }} from <a href='{{ url_for('edit_group', id=value[1] ) | safe }}'>group</a> at depth {{ value[2] }}
			{% if value[3] | length > 0 %}</li>
				Overrode:
				<ul>
					{% for overridden_value, overridden_group, overriden_depth in value[3] %}
						<li> {{ key }} = {{ overridden_value }} from <a href='{{ url_for('edit_group', id=overridden_group ) | safe }}'>group</a> at depth {{ overriden_depth }} </li>
					{% endfor %}
				</ul>
			{% endif %}
		{% endif %}
	{% endfor %}
</ul>