{% extends "base.j2" %}
{% block title %}
New CA
{% endblock %}
{% block content %}

    <h3>New CA</h3>
    <form action="" method="post">
        {{ form.hidden_tag() }}
        <br \>
            {{ form.name.label }}<br>
            {{ form.name(size=32) }}
        <br \>
        <br \>
            {{ form.public_key.label }}<br>
            {{ form.public_key }}
        <br \>
        <br \>
            {{ form.private_key.label }}<br>
            {{ form.private_key }}
        <br \>
        <br \>
            {{ form.note.label }}<br>
            {{ form.note(size=32) }}
        <br \>
        <br \>
            {{ form.submit() }}<br \>
    </form>

{% endblock %}
